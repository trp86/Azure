		{
			"name": "[concat(parameters('factoryName'), '/pl_his_MilesUK')]",
			"type": "Microsoft.DataFactory/factories/pipelines",
			"apiVersion": "2018-06-01",
			"properties": {
				"activities": [
					{
						"name": "InsertPackageExecutionLog",
						"type": "Script",
						"dependsOn": [],
						"policy": {
							"timeout": "0.12:00:00",
							"retry": 0,
							"retryIntervalInSeconds": 30,
							"secureOutput": false,
							"secureInput": false
						},
						"userProperties": [],
						"linkedServiceName": {
							"referenceName": "ls_dwh_mgt",
							"type": "LinkedServiceReference"
						},
						"typeProperties": {
							"scripts": [
								{
									"type": "Query",
									"text": "DECLARE @PackageExecutionLogId AS INT\n, @PackageName AS VARCHAR(250) = 'TestPackage'\n, @BatchRunId AS INT = 100\n, @PackageID AS VARCHAR(250) = 'TestPackageId'\n, @ETLType AS VARCHAR(50) = 'DeltaLoad'\n, @PValidFromDate AS VARCHAR(50) = '31/08/2022'\n, @SSISDBPackageExecutionId INT = 200\n\nEXEC @PackageExecutionLogId = Audit.USP_InsertPackageExecutionLog @PackageName,@BatchRunId,@PackageID,@ETLType,@SSISDBPackageExecutionId\n\nSELECT PackageExecutionLogId = @PackageExecutionLogId\n ,ValidFromDate = ISNULL(TRY_CAST(TRIM(@PValidFromDate) AS DATETIME),GETDATE())"
								}
							]
						}
					}
				],
				"policy": {
					"elapsedTimeMetric": {},
					"cancelAfter": {}
				},
				"annotations": []
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/linkedServices/ls_dwh_mgt')]"
			]
		},
