							},
							"password": {
								"value": "@pipeline().parameters.password",
								"type": "Expression"
							}
						}
					},
					"firstRowOnly": false
				}
			},
			{
				"name": "Loop through metadata",
				"type": "ForEach",
				"dependsOn": [
					{
						"activity": "Read Metadata",
						"dependencyConditions": [
							"Succeeded"
						]
					}
				],
				"userProperties": [],
				"typeProperties": {
					"items": {
						"value": "@activity('Read Metadata').output.value",
						"type": "Expression"
					},
					"isSequential": true,
					"activities": [
						{
							"name": "CopyToStagingTable",
							"type": "Copy",
							"dependsOn": [
								{
									"activity": "CheckStub_1",
									"dependencyConditions": [
										"Succeeded"
									]
								}
							],
							"policy": {
								"timeout": "0.00:10:00",
								"retry": 0,
								"retryIntervalInSeconds": 30,
								"secureOutput": false,
								"secureInput": false
							},
							"userProperties": [],
							"typeProperties": {
								"source": {
									"type": "AzureSqlSource",
									"additionalColumns": [
										{
											"name": "MGT_InsertBatchRunId",
											"value": "200"
										},
										{
											"name": "MGT_InsertDate",
											"value": {
												"value": "@formatDateTime(utcnow(), 'dd-MMM-yyyy')",
												"type": "Expression"
											}
										}
									],
									"sqlReaderQuery": {
										"value": "@{item().src_extract_query}",
										"type": "Expression"
									},
									"queryTimeout": "02:00:00",
									"partitionOption": "None"
								},
								"sink": {
									"type": "AzureSqlSink",
									"preCopyScript": {
										"value": "@{item().tgt_truncate_query}",
										"type": "Expression"
									},
									"writeBehavior": "insert",
									"sqlWriterUseTableLock": false,
									"disableMetricsCollection": false
								},
								"enableStaging": false,
								"enableSkipIncompatibleRow": true,
								"validateDataConsistency": true,
								"translator": {
									"type": "TabularTranslator",
									"typeConversion": true,
									"typeConversionSettings": {
										"allowDataTruncation": false,
										"treatBooleanAsNumber": false
									}
								}
							},
							"inputs": [
								{
									"referenceName": "ds_dynamic_tbl",
									"type": "DatasetReference",
									"parameters": {
										"schema": {
											"value": "@{item().src_schema}",
											"type": "Expression"
										},
										"table": "@{item().src_tbl}",
										"database": "@{item().src_db}",
										"dbservername": {
											"value": "@pipeline().parameters.servername",
											"type": "Expression"
										},
										"username": {
											"value": "@pipeline().parameters.username",
											"type": "Expression"
										},
										"password": {
											"value": "@pipeline().parameters.password",
											"type": "Expression"
										}
									}
								}
							],
							"outputs": [
								{
									"referenceName": "ds_dynamic_tbl",
									"type": "DatasetReference",
									"parameters": {
										"schema": "@{item().tgt_schema}",
										"table": "@{item().tgt_tbl}",
										"database": "@{item().tgt_db}",
										"dbservername": {
											"value": "@pipeline().parameters.servername",
											"type": "Expression"
										},
										"username": {
											"value": "@pipeline().parameters.username",
											"type": "Expression"
										},
										"password": {
											"value": "@pipeline().parameters.password",
											"type": "Expression"
										}
									}
								}
							]
						},
						{
							"name": "USP_InsertPackageExecutionLog",
							"type": "Script",
							"dependsOn": [],
							"policy": {
								"timeout": "0.00:05:00",
								"retry": 0,
								"retryIntervalInSeconds": 30,
								"secureOutput": false,
								"secureInput": false
							},
							"userProperties": [],
							"linkedServiceName": {
								"referenceName": "ls_generic",
								"type": "LinkedServiceReference",
								"parameters": {
									"databasename": "dwh_mgt",
									"username": "trp86",
									"password": "Daimler2022",
									"dbservername": "milesdwh.database.windows.net"
								}
							},
							"typeProperties": {
								"scripts": [
									{
										"parameters": [
											{
												"name": "PackageName",
												"type": "String",
												"value": "placeholder",
												"direction": "Input"
											},
											{
												"name": "BatchRunId",
												"type": "Int64",
												"value": "211",
												"direction": "Input"
											},
											{
												"name": "PackageGUID",
												"type": "String",
												"value": "placeholder",
												"direction": "Input"
											},
											{
												"name": "ETLType",
												"type": "String",
												"value": "placeholder",
												"direction": "Input"
											},
											{
												"name": "SSISDBPackageExecutionId",
												"type": "Int64",
												"value": "32",
												"direction": "Input"
											},
											{
												"name": "RC",
												"type": "Int64",
												"value": "45",
												"direction": "Output"
											}
										],
										"type": "Query",
										"text": "\n\nEXECUTE @RC = [Audit].[USP_InsertPackageExecutionLog] \n   @PackageName\n  ,@BatchRunId\n  ,@PackageGUID\n  ,@ETLType\n  ,@SSISDBPackageExecutionId\n\nselect @RC as PackageExecutionLogId"
									}
								]
							}
						},
						{
							"name": "SetPackageExecutionLogId",
							"type": "SetVariable",
							"dependsOn": [
								{
									"activity": "USP_InsertPackageExecutionLog",
									"dependencyConditions": [
										"Succeeded"
									]
								}
							],
							"userProperties": [],
							"typeProperties": {
								"variableName": "V_PackageExecutionLogId",
								"value": {
									"value": "@string(activity('USP_InsertPackageExecutionLog').output.resultSets[0].rows[0].PackageExecutionLogId3)",
									"type": "Expression"
								}
							}
						},
						{
							"name": "SetPipelineRunId",
							"type": "SetVariable",
							"dependsOn": [
								{
									"activity": "USP_InsertPackageExecutionLog",
									"dependencyConditions": [
										"Succeeded"
									]
								}
							],
							"userProperties": [],
							"typeProperties": {
								"variableName": "V_PipelineRunId",
								"value": {
									"value": "@pipeline().RunId",
									"type": "Expression"
								}
							}
						},
						{
							"name": "SetNumberOfRowsRead",
							"type": "SetVariable",
							"dependsOn": [
								{
									"activity": "CopyToStagingTable",
									"dependencyConditions": [
										"Succeeded"
									]
								}
							],
							"userProperties": [],
							"typeProperties": {
								"variableName": "V_NumberOfRowsRead",
								"value": {
									"value": "@replace(string(activity('CopyToStagingTable').output.rowsRead),'\\n','')\n\n",
									"type": "Expression"
								}
							}
						},
						{
							"name": "CheckStub_1",
							"type": "Wait",
							"dependsOn": [
								{
									"activity": "SetPackageExecutionLogId",
									"dependencyConditions": [
										"Succeeded"
									]
								},
								{
									"activity": "SetPipelineRunId",
									"dependencyConditions": [
										"Succeeded"
									]
								}
							],
							"userProperties": [],
							"typeProperties": {
								"waitTimeInSeconds": 1
							}
						},
						{
							"name": "SetNumberOfRowsCopied",
							"type": "SetVariable",
							"dependsOn": [
								{
									"activity": "CopyToStagingTable",
									"dependencyConditions": [
										"Succeeded"
									]
								}
							],
							"userProperties": [],
							"typeProperties": {
								"variableName": "V_NumberOfRowsCopied",
								"value": {
									"value": "@replace(string(activity('CopyToStagingTable').output.rowsCopied),'\\n','')\n\n",
									"type": "Expression"
								}
							}
						},
						{
							"name": "CheckStub_2",
							"type": "Wait",
							"dependsOn": [
								{
									"activity": "SetNumberOfRowsRead",
									"dependencyConditions": [
										"Succeeded"
									]
								},
								{
									"activity": "SetNumberOfRowsCopied",
									"dependencyConditions": [
										"Succeeded"
									]
								}
							],
							"userProperties": [],
							"typeProperties": {
								"waitTimeInSeconds": 1
							}
						},
						{
							"name": "SetFailureErrorMessage",
							"type": "SetVariable",
							"dependsOn": [
								{
									"activity": "MatchRowsReadAndCopied",
									"dependencyConditions": [
										"Failed",
										"Skipped"
									]
								}
							],
							"userProperties": [],
							"typeProperties": {
								"variableName": "V_NumberOfRowsRead",
								"value": {
									"value": "@concat(\n\tactivity('USP_InsertPackageExecutionLog')?.Error?.message,\n\t'|',\n\tactivity('SetPackageExecutionLogId')?.Error?.message,\n\t'|',\n\tactivity('SetPipelineRunId')?.Error?.message,\n\t'|',\n\tactivity('CopyToStagingTable')?.Error?.message,\n\t'|',\n\tactivity('SetNumberOfRowsRead')?.Error?.message,\n\t'|',\n\tactivity('SetNumberOfRowsCopied')?.Error?.message,\n\t'|',\n\tactivity('MatchRowsReadAndCopied')?.Error?.message\n\t) ",
									"type": "Expression"
								}
							}
						},
						{
							"name": "MatchRowsReadAndCopied",
							"type": "IfCondition",
							"dependsOn": [
								{
									"activity": "CheckStub_2",
									"dependencyConditions": [
										"Completed"
									]
								}
							],
							"userProperties": [],
							"typeProperties": {
								"expression": {
									"value": "@equals(variables('V_NumberOfRowsRead'),variables('V_NumberOfRowsCopied'))",
									"type": "Expression"
								},
								"ifFalseActivities": [
									{
										"name": "Fail1",
										"type": "Fail",
										"dependsOn": [],
										"userProperties": [],
										"typeProperties": {
											"message": {
												"value": "@string('Rows Copied is not equal to Rows read.')",
												"type": "Expression"
											},
											"errorCode": "500"
										}
									}
								],
								"ifTrueActivities": [
									{
										"name": "USP_UpdatePackageExecutionLog",
										"type": "Script",
										"dependsOn": [],
										"policy": {
											"timeout": "0.00:05:00",
											"retry": 0,
											"retryIntervalInSeconds": 30,
											"secureOutput": false,
											"secureInput": false
										},
										"userProperties": [],
										"linkedServiceName": {
											"referenceName": "ls_generic",
											"type": "LinkedServiceReference",
											"parameters": {
												"databasename": "dwh_mgt",
												"username": "trp86",
												"password": "Daimler2022",
												"dbservername": "milesdwh.database.windows.net"
											}
										},
										"typeProperties": {
											"scripts": [
												{
													"parameters": [
														{
															"name": "PackageExecutionLogId",
															"type": "Int64",
															"value": {
																"value": "@int(variables('V_packageexecid'))",
																"type": "Expression"
															},
															"direction": "Input"
														},
														{
															"name": "NumberOfRowsReadSource",
															"type": "Int64",
															"value": {
																"value": "@int(variables('V_NumberOfRowsRead'))",
																"type": "Expression"
															},
															"direction": "Input"
														},
														{
															"name": "NumberOfRowsReadTarget",
															"type": "Int64",
															"value": {
																"value": "@int(variables('V_NumberOfRowsRead'))",
																"type": "Expression"
															},
															"direction": "Input"
														},
														{
															"name": "NumberOfRowsUpdate",
															"type": "Int64",
															"value": {
																"value": "int(0)",
																"type": "Expression"
															},
															"direction": "Input"
														},
														{
															"name": "NumberOfRowsInsert",
															"type": "Int64",
															"value": {
																"value": "int(0)",
																"type": "Expression"
															},
															"direction": "Input"
														},
														{
															"name": "NumberOfRowsDelete",
															"type": "Int64",
															"value": {
																"value": "int(0)",
																"type": "Expression"
															},
															"direction": "Input"
														},
														{
															"name": "NumberOfRowsError",
															"type": "Int64",
															"value": {
																"value": "int(0)",
																"type": "Expression"
															},
															"direction": "Input"
														},
														{
															"name": "CompletedInd",
															"type": "Int64",
															"value": {
																"value": "int(1)",
																"type": "Expression"
															},
															"direction": "Input"
														},
														{
															"name": "FailedInd",
															"type": "Int64",
															"value": {
																"value": "int(0)",
																"type": "Expression"
															},
															"direction": "Input"
														},
														{
															"name": "FilterValue",
															"type": "String",
															"value": {
																"value": "@string('NoFilterValue')",
																"type": "Expression"
															},
															"direction": "Input"
														},
														{
															"name": "IncrementValue",
															"type": "String",
															"value": {
																"value": "@string('NoIncrementValue')",
																"type": "Expression"
															},
															"direction": "Input"
														},
														{
															"name": "IncrementalLoadInd",
															"type": "Int64",
															"value": {
																"value": "int(0)",
																"type": "Expression"
															},
															"direction": "Input"
														},
														{
															"name": "ApplyFilterInd",
															"type": "Int64",
															"value": {
																"value": "int(0)",
																"type": "Expression"
															},
															"direction": "Input"
														},
														{
															"name": "ValidFromDate",
															"type": "Datetime",
															"value": {
																"value": "@utcnow()",
																"type": "Expression"
															},
															"direction": "Input"
														}
													],
													"type": "Query",
													"text": "EXECUTE Audit.USP_UpdatePackageExecutionLog \n   @PackageExecutionLogId    -- 1\n   ,@NumberOfRowsReadSource  -- 2\n   ,@NumberOfRowsReadTarget  -- 3\n   ,@NumberOfRowsUpdate      -- 4 \n   ,@NumberOfRowsInsert      -- 5\n   ,@NumberOfRowsDelete      -- 6\n   ,@NumberOfRowsError       -- 7\n   ,@CompletedInd            -- 8 \n   ,@FailedInd               -- 9\n   ,@FilterValue             -- 10 \n   ,@IncrementValue          -- 11\n   ,@IncrementalLoadInd      -- 12\n   ,@ApplyFilterInd          -- 13\n   ,@ValidFromDate           -- 14"
												}
											]
										}
									}
								]
							}
						}
					]
				}
			}
		],
		"parameters": {
			"servername": {
				"type": "string",
				"defaultValue": "milesdwh.database.windows.net"
			},
			"username": {
				"type": "string",
				"defaultValue": "trp86"
			},
			"password": {
				"type": "string",
				"defaultValue": "Daimler2022"
			},
			"sourcedbname": {
				"type": "string",
				"defaultValue": "src"
			},
			"targetdbname": {
				"type": "string",
				"defaultValue": "dwh_stg"
			},
			"sourcedbschema": {
				"type": "string",
				"defaultValue": "dbo"
			},
			"targetdbschema": {
				"type": "string",
				"defaultValue": "dbo"
			},
			"extract_sp": {
				"type": "string",
				"defaultValue": "dbo.ExtractQueries"
			},
			"sp_InsertPackageExecutionLog": {
				"type": "string",
				"defaultValue": "USP_InsertPackageExecutionLog"
			},
			"mgtdbname": {
				"type": "string",
				"defaultValue": "dwh_mgt"
			},
			"auditschema": {
				"type": "string",
				"defaultValue": "Audit"
			}
		},
		"variables": {
			"V_packageexecid": {
				"type": "String",
				"defaultValue": "placeholder"
			},
			"V_PackageExecutionLogId": {
				"type": "String",
				"defaultValue": "placeholder"
			},
			"V_PipelineRunId": {
				"type": "String",
				"defaultValue": "placeholder"
			},
			"V_NumberOfRowsRead": {
				"type": "String",
				"defaultValue": "placeholder"
			},
			"V_NumberOfRowsCopied": {
				"type": "String",
				"defaultValue": "placeholder"
			},
			"V_FailureErrorMessage": {
				"type": "String",
				"defaultValue": "placeholder"
			}
		},
		"annotations": []
	}
