								"retryIntervalInSeconds": 30,
								"secureOutput": false,
								"secureInput": false
							},
							"userProperties": [],
							"typeProperties": {
								"source": {
									"type": "AzureSqlSource",
									"additionalColumns": [
										{
											"name": "MGT_InsertBatchRunId",
											"value": "200"
										},
										{
											"name": "MGT_InsertDate",
											"value": {
												"value": "@formatDateTime(utcnow(), 'dd-MMM-yyyy')",
												"type": "Expression"
											}
										}
									],
									"sqlReaderQuery": {
										"value": "@{item().src_extract_query}",
										"type": "Expression"
									},
									"queryTimeout": "02:00:00",
									"partitionOption": "None"
								},
								"sink": {
									"type": "AzureSqlSink",
									"preCopyScript": {
										"value": "@{item().tgt_truncate_query}",
										"type": "Expression"
									},
									"writeBehavior": "insert",
									"sqlWriterUseTableLock": false,
									"disableMetricsCollection": false
								},
								"enableStaging": false,
								"enableSkipIncompatibleRow": true,
								"validateDataConsistency": true,
								"translator": {
									"type": "TabularTranslator",
									"typeConversion": true,
									"typeConversionSettings": {
										"allowDataTruncation": false,
										"treatBooleanAsNumber": false
									}
								}
							},
							"inputs": [
								{
									"referenceName": "ds_dynamic_tbl",
									"type": "DatasetReference",
									"parameters": {
										"schema": {
											"value": "@{item().src_schema}",
											"type": "Expression"
										},
										"table": "@{item().src_tbl}",
										"database": "@{item().src_db}",
										"dbservername": {
											"value": "@pipeline().parameters.servername",
											"type": "Expression"
										},
										"username": {
											"value": "@pipeline().parameters.username",
											"type": "Expression"
										},
										"password": {
											"value": "@pipeline().parameters.password",
											"type": "Expression"
										}
									}
								}
							],
							"outputs": [
								{
									"referenceName": "ds_dynamic_tbl",
									"type": "DatasetReference",
									"parameters": {
										"schema": "@{item().tgt_schema}",
										"table": "@{item().tgt_tbl}",
										"database": "@{item().tgt_db}",
										"dbservername": {
											"value": "@pipeline().parameters.servername",
											"type": "Expression"
										},
										"username": {
											"value": "@pipeline().parameters.username",
											"type": "Expression"
										},
										"password": {
											"value": "@pipeline().parameters.password",
											"type": "Expression"
										}
									}
								}
							]
						},
						{
							"name": "USP_InsertPackageExecutionLog",
							"type": "Script",
							"dependsOn": [],
							"policy": {
								"timeout": "0.00:05:00",
								"retry": 0,
								"retryIntervalInSeconds": 30,
								"secureOutput": false,
								"secureInput": false
							},
							"userProperties": [],
							"linkedServiceName": {
								"referenceName": "ls_generic",
								"type": "LinkedServiceReference",
								"parameters": {
									"databasename": "dwh_mgt",
									"username": "trp86",
									"password": "Daimler2022",
									"dbservername": "milesdwh.database.windows.net"
								}
							},
							"typeProperties": {
								"scripts": [
									{
										"parameters": [
											{
												"name": "PackageName",
												"type": "String",
												"value": "placeholder",
												"direction": "Input"
											},
											{
												"name": "BatchRunId",
												"type": "Int64",
												"value": "211",
												"direction": "Input"
											},
											{
												"name": "PackageGUID",
												"type": "String",
												"value": "placeholder",
												"direction": "Input"
											},
											{
												"name": "ETLType",
												"type": "String",
												"value": "placeholder",
												"direction": "Input"
											},
											{
												"name": "SSISDBPackageExecutionId",
												"type": "Int64",
												"value": "32",
												"direction": "Input"
											},
											{
												"name": "RC",
												"type": "Int64",
												"value": "45",
												"direction": "Output"
											}
										],
										"type": "Query",
										"text": "\n\nEXECUTE @RC = [Audit].[USP_InsertPackageExecutionLog] \n   @PackageName\n  ,@BatchRunId\n  ,@PackageGUID\n  ,@ETLType\n  ,@SSISDBPackageExecutionId\n\nselect @RC as PackageExecutionLogId"
									}
								]
							}
						},
						{
							"name": "SetPackageExecutionLogId",
							"type": "SetVariable",
							"dependsOn": [
