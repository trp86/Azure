					"firstRowOnly": false
				}
			},
			{
				"name": "Loop through metadata",
				"type": "ForEach",
				"dependsOn": [
					{
						"activity": "Read Metadata",
						"dependencyConditions": [
							"Succeeded"
						]
					}
				],
				"userProperties": [],
				"typeProperties": {
					"items": {
						"value": "@activity('Read Metadata').output.value",
						"type": "Expression"
					},
					"isSequential": true,
					"activities": [
						{
							"name": "CopyToStagingTable",
							"type": "Copy",
